{
    "name": "boilerplate",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.17.1",
        "moment": "^2.21.0",
        "node-sass-chokidar": "0.0.3",
        "npm-run-all": "^4.1.2",
        "react": "^16.2.0",
        "react-color": "^2.14.1",
        "react-datepicker": "^1.3.0",
        "react-dates": "^16.3.6",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "react-scripts": "1.0.17",
        "redux": "^3.7.2"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/assets/styles/main.scss -o public/css/",
        "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles/main.scss -o public/css/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "REACT_APP_ENV=develop npm-run-all -p watch-css start-js",
        "build-js": "react-scripts build",
        "build": "REACT_APP_ENV=production npm-run-all build-css build-js",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "immutable": "^3.8.2",
        "redux-freeze": "^0.1.5",
        "redux-thunk": "^2.2.0",
        "vinyl-source-stream": "^1.1.0"
    }
}
